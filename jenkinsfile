pipeline {
    agent { node { label 'node1' } }
    triggers { pollSCM('* * * * *') }

    stages {
        stage('vcs') {
            steps {
             git url: 'https://github.com/nagarjunaduggireddy/openmrs-core.git', 
             branch: 'SPRINT-1-DEV'
            }
        }
        stage('build') {
            steps {
                sh "mvn package"
            }
        }
    }
}


