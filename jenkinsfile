pipeline {
    agent { node { label 'MVN3' } }
	triggers { cron('H 18 * * 1-5') }
    stages {
        stage('vcs') {
            steps {
             git url: 'https://github.com/nagarjunaduggireddy/openmrs-core.git', branch: 'SPRINT-1-REL'
            }
        }
        stage('build') {
            steps {
                sh "mvn package"
            }
        }
    }
}
