pipeline {
    agent { label 'JDK11'} 
    triggers { pollSCM('* * * * *) }

    stages {
        stage('vcs') {
            steps {
             git url: 'https://github.com/nagarjunaduggireddy/openmrs-core.git', branch: 'master'
            }
        }
        stage('build') {
            steps {
                sh "mvn package"
            }
        }
    }
}